#include<bits/stdc++.h>
using namespace std;

void halfsubtractor(int A, int B){
	int diff, borrow;
	diff=A^B;
	A=not(A);
	borrow=A&B;
	
	cout<<"Difference:"<<diff<<endl;
	cout<<"Borrow:"<<borrow<<endl;
}

int main(){
	int A,B;
	cin>>A>>B;
	cout<<"A:"<<A<<endl;
	cout<<"B:"<<B<<endl;
	
	halfsubtractor(A,B);
	return 0;
}
